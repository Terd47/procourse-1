  {{>navbar}}

  <div class="container-lg">
    <div class="row">
      <div class="col-md-12">
        
        <div class="card-colums" id="project-container">
             <!-- Cards -->
             <div class="column" >
              {{#each project}}
                <div class="card prod">
                    <div class="card-body">

                    <h3><a class="card-title title" href="/projectdetails/{{id}}">{{title}}</a></h3><hr>
                    <h6 class="card-subtitle mb-2 text-muted desc">Project Description</h6>
                    <p class="card-text">{{description}}</p>
                    {{!-- Collab Section --}}
                      <a type="button" class="viewCollab" data-value="{{id}}">View Collaborators</a>
                      <div class="collabSection{{id}}">
                        
                      </div>
                    <a href="#" class="card-link">Created By</a>
                    <a href="/api/update/{{id}}" data-value="{{id}}" class="updateProj"> Update</a>
                     <button class="del btn btn-primary" id="deleteProject" data-id="{{UserId}}" data-value="{{id}}">delete</button>

                    </div>
                </div>
              
              {{/each}}
            </div>
        </div>
      </div>
    </div>
  </div>

   <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script type="text/javascript" src="js/feeds.js"></script>
  
  {{>footer}}
  